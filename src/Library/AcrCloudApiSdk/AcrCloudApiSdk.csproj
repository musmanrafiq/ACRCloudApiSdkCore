<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>AcrCloudApiSdk</PackageId>
    <Title>AcrCloudApiSdk</Title>
    <Version>1.0.1</Version>
    <Authors>NetTech</Authors>
    <Company>NetTech</Company>
    <Description>AcrCloudApiSdk is wrapper library around AcrCloud API's.</Description>
    <RepositoryUrl>https://github.com/musmanrafiq/ACRCloudApiSdkCore</RepositoryUrl>
    <PackageTags>AcrCloudAPISdk, AcrCloudApiWrapper</PackageTags>
    <PackageReleaseNotes>Currently we have AcrCloud's monitoring database related channels list support only.</PackageReleaseNotes>
    <NeutralLanguage>en</NeutralLanguage>
    <SignAssembly>false</SignAssembly>
  </PropertyGroup>

  <PropertyGroup>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>
</Project>
