<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>AcrCloudApiSdk</PackageId>
    <Title>AcrCloudApiSdk</Title>
    <Version>1.1.0</Version>
    <Authors>M.Usman Rafiq</Authors>
    <Company>M.Usman Rafiq</Company>
    <Description>AcrCloudApiSdk is wrapper library around AcrCloud API's.</Description>
    <RepositoryUrl>https://github.com/musmanrafiq/ACRCloudApiSdkCore</RepositoryUrl>
    <PackageTags>AcrCloudAPISdk, AcrCloudApiWrapper</PackageTags>
    <PackageReleaseNotes>Get a Channel report on particular date</PackageReleaseNotes>
    <NeutralLanguage>en</NeutralLanguage>
    <SignAssembly>false</SignAssembly>
  </PropertyGroup>

  <PropertyGroup>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <FileVersion>1.1.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Executables\acrcloud_extr_win.exe">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="Files\Fingerprints\readme.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>
